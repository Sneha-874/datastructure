#include<stdio.h>

void IncrementByOne(int n, int arr[n]) {
    for(int i = 0; i < n; i++) {
        arr[i] = arr[i] + 1;
    }
}

int main() {
    int size;
    printf("Enter the size of array: ");
    scanf("%d", &size);

    int arr[size];
    printf("Enter array elements:\n");
    
    for(int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    IncrementByOne(size, arr);

    printf("Array after incrementing each element by one:\n");
    for(int i = 0; i < size; i++) {
        printf("%d\t", arr[i]);
    }

    return 0;
}
