//Inserting a new node at start and printing the new linked list
#include<stdio.h>
#include<stdlib.h>
struct node
{
    int data;
    struct node* next;
};
void insertAtStart(struct node *ptr , int num){
    
   struct node *new_node;
   new_node = (struct node*)malloc(sizeof(struct node));
   new_node->data = num;
   new_node->next = ptr;
   ptr = new_node;
   while(ptr!=NULL){
        printf("Element:- %d\t" , ptr->data);
        ptr = ptr->next;
    }
}
void print(struct node*ptr){
    while(ptr!=NULL){
        printf("Element:- %d\t" , ptr->data);
        ptr = ptr->next;
    }
}


int main()
{
    int num;
    struct node *ptr;
    struct node *start;
    struct node *second;
    struct node *third;
    struct node *fourth;
    start= (struct node*)malloc(sizeof(struct node));
    second = (struct node*)malloc(sizeof(struct node));
    third = (struct node*)malloc(sizeof(struct node));
    fourth = (struct node*)malloc(sizeof(struct node));
   start->data=7;
   start->next=second;
    second->data = 5;
    second->next = third;
    third->data = 12;
    third->next = fourth;
    fourth->data=56;
    fourth->next = NULL;
   ptr = start;
   printf("Linked list before insertion:-\n");
   print(ptr);
    printf("\nEnter number to be inserted at start:- ");
    scanf("%d" , &num);
   printf("\nLinked list after insertion:-\n");
   insertAtStart(start , num);

    
    return 0;
}
