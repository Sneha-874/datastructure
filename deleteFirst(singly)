//Deleting a node from starting
#include<stdio.h>
#include<stdlib.h>
struct node
{
    int data;
    struct node* next;
};
void deleteStart(struct node *ptr ){
    struct node *temp;
    temp = ptr;
    ptr=ptr->next;
    free(temp);
    while(ptr!=NULL){
        printf("Element:- %d\t" , ptr->data);
        ptr = ptr->next;
    }
}
void print(struct node*ptr){
    while(ptr!=NULL){
        printf("Element:- %d\t" , ptr->data);
        ptr = ptr->next;
    }
}
int main()
{
    struct node *ptr;
    struct node *temp;
    struct node *start;
    struct node *second;
    struct node *third;
    struct node *fourth;
    start= (struct node*)malloc(sizeof(struct node));
    second = (struct node*)malloc(sizeof(struct node));
    third = (struct node*)malloc(sizeof(struct node));
    fourth = (struct node*)malloc(sizeof(struct node));
   start->data=7;
   start->next=second;
    second->data = 5;
    second->next = third;
    third->data = 12;
    third->next = fourth;
    fourth->data=56;
    fourth->next = NULL;
   ptr = start;
   
   printf("Linked list before deletion:-\n");
   print(ptr);
   printf("\nLinked list after deletion:-\n");
   deleteStart(ptr);
    return 0;
}
