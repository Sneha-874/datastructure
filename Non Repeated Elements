//Finding non repeated elements in an array in c++
#include<iostream>
#include<vector>

using namespace std;

int main() {
    vector<int> nonRepeatedElements;
    int size;
    int flag;

    cout << "Enter the size of array: ";
    cin >> size;

    
    vector<int> arr(size);

    cout << "Enter the array elements: " << endl;
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }

    for (int i = 0; i < size; i++) {
        flag = 0;

        for (int j = 0; j < size; j++) {
            if (i != j && arr[i] == arr[j]) {
                flag = 1;
                break;
            }
        }

        if (flag == 0) {
            nonRepeatedElements.push_back(arr[i]);
        }
    }

    cout << "Non Repeated Elements of array are: " << endl;
    for (int i = 0; i < nonRepeatedElements.size(); i++) {
        cout << nonRepeatedElements[i] << " ";
    }

    return 0;
}
